{
  "name": "@opensbbl/cli",
  "version": "1.0.0",
  "description": "OpenSBBL CLI - Interactive tool for generating AI-optimized software blueprints",
  "main": "dist/index.js",
  "bin": {
    "sbbl": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsup src/index.ts --format esm --watch --onSuccess \"node dist/index.js\"",
    "build": "tsup src/index.ts --format esm --clean && cpy src/templates/**/* dist/templates",
    "start": "node dist/index.js",
    "test": "vitest"
  },
  "keywords": [
    "sbbl",
    "opensbbl",
    "cli",
    "blueprint",
    "ai",
    "development",
    "methodology",
    "agile"
  ],
  "author": "OpenSBBL Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DiogenesAI/OpenSBBL.git",
    "directory": "sbbl-cli"
  },
  "bugs": {
    "url": "https://github.com/DiogenesAI/OpenSBBL/issues"
  },
  "homepage": "https://github.com/DiogenesAI/OpenSBBL#readme",
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "cpy-cli": "^6.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "type": "module"
}